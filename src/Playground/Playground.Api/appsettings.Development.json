{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "CorsOptions": {
    "AllowAll": true
  },
  "OpenApiOptions": {
    "Enabled": true
  },
  "MultitenancyOptions": {
    "RunTenantMigrationsOnStartup": false,
    "AutoProvisionOnStartup": true
  },
  "OpenTelemetryOptions": {
    "Exporter": {
      "Otlp": {
        "Enabled": false
      }
    }
  }
}
